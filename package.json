{
  "name": "create-aggain",
  "version": "1.0.0",
  "description": "Um gerador moderno de BFF com suporte para Express e Koa",
  "bin": {
    "create-aggain": "bin/agni"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsx watch src/index.ts",
    "build": ".\\node_modules\\.bin\\tsc",
    "start": "node dist\\index.js",
    "prepublishOnly": ".\\node_modules\\.bin\\tsc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kaiquewdev/aggain.git"
  },
  "keywords": [
    "scaffolding",
    "generator",
    "bff",
    "express",
    "koa",
    "typescript",
    "create-app"
  ],
  "author": "Kaique da Silva <kaique.silva.dev@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kaiquewdev/aggain/issues"
  },
  "homepage": "https://github.com/kaiquewdev/aggain",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "graceful-fs": "^4.2.11",
    "helmet": "^7.1.0",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.1",
    "koa-compress": "^5.1.1",
    "koa-helmet": "^7.0.2",
    "mkdirp": "^3.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/koa": "^2.13.12",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-compress": "^4.0.6",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
